{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": { "zeroConfig": true }
    }
  ],
  "routes": [
    {
      "src": "/(favicon.ico|style.css)",
      "dest": "/templates/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}